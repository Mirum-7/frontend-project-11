{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "google",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "indent": [
        "error",
        "tab", 
        { 
					"SwitchCase": 1,
					"FunctionExpression": {"parameters": "first"} 
				}
    	],
      "no-tabs":"off",
			"object-curly-spacing": ["error", "always"],
			"max-len": "off",
      "require-jsdoc": "off"
    }
}
